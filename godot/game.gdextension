[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../game/target/debug/libgame.so"
linux.release.x86_64 =   "res://../game/target/release/libgame.so"
windows.debug.x86_64 =   "res://../game/target/x86_64-pc-windows-gnu/release/game.dll"
windows.release.x86_64 = "res://../game/target/x86_64-pc-windows-gnu/release/game.dll"
macos.debug =            "res://../game/target/debug/libgame.dylib"
macos.release =          "res://../game/target/release/libgame.dylib"
macos.debug.arm64 =      "res://../game/target/debug/libgame.dylib"
macos.release.arm64 =    "res://../game/target/release/libgame.dylib"